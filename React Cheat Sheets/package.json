    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PolarisTLX/ReactJS-Basics.git"
  },
